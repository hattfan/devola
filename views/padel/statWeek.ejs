<% include ./partials/header.ejs %>
<link rel="stylesheet" href="css/stats.css">
    <!-- Register Page -->
    <div class="container">
        <div class="register">
            <h1>Veckoligan</h1>
            <% include ./partials/buttons.ejs %>
            <!-- <div class="chart" id="vecko_chart" ></div> -->
            <!--<hr>-->
            <table class="table table-dark table-striped">
                <thead>
                    <tr>
                        <th>Namn</th>
                        <th>SM</th>
                        <th>VM</th>
                        <th>FM</th>
                        <th>%</th>
                        <th>Mål</th>
                        <th>Vikt</th>
                    </tr>
                </thead>
                <tbody>
                    <% dataname.forEach(player => { %>
                    <tr>
                        <td><%= player.Spelare %></td>
                        <td><%= player.SpeladeMatcher%></td>
                        <td><%= player.Vinster %></td>
                        <td><%= player.Förluster %></td>
                        <td><%= Math.round(player.Vinster/(player.Förluster+player.Vinster)*1000)/10 %> %</td>
                        <td><%= player.GjordaMål-player.InsläpptaMål %></td>
                        <td><%= Math.round(player.Viktning*100)/100 %></td>
                        <!--<td>Math.round(((Math.pow(player.Vinster/((player.Förluster) + (player.Vinster)),3) * player.Vinster) + Math.round(player.GjordaMål-player.InsläpptaMål)*0.001)*100)/100</td>-->
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
    <!-- //Register Page -->

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
        <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
        <script src="js/weekly_chart.js"></script>
</body>

</html>