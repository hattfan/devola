<% include ./partials/header.ejs %>
    <link rel="stylesheet" href="css/stats.css">
    <!-- Register Page -->
    <div class="container">
        <div class="register">
            <h1>SpelarAdmin</h1>
            <table class="table table-dark table-striped">
                <thead>
                    <tr>
                        <th>Namn</th>
                    </tr>
                </thead>
                <tbody>
                    <% data.forEach(player => { %>
                        <tr>
                            <td><a class="btn btn-danger noMargin" href="/padel/remove/<%= player['_id'] %>" role="button">Inaktivera</a> <a class="btn btn-success noMargin" href="/padel/activate/<%= player['_id'] %>" role="button">Aktivera</a> <%= player.Spelare %> - <%= player['Aktiv']===true?'Aktiv':'Inaktiv' %> </td> </td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
    <!-- //Register Page -->

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
    </body>

    </html>