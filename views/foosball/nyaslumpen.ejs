<% include ./partials/header.ejs %>
<link rel="stylesheet" href="css/stats.css">
<link rel="stylesheet" href="css/nyaslumpen.css">

<!-- Register Page -->
<div class="container">
    <div class="register">
        <div id="slump-btn" class="btn btn-success btn-block">Slumpa nu</div>
        <br>
        <div class="contain">
            <div class="player-container">
                <% data.forEach(player => { %>
                <div class="play-choice">
                    <div class="choice-picture"><i class="fas fa-user fa-4x"></i></div>
                    <div>
                        <%= player.Spelare %>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </div>
</div>
<!-- //Register Page -->

<!-- Optional JavaScript -->
<!-- jQuery first, then Popper.js, then Bootstrap JS -->
<script src="js/nyaslumpen.js"></script>
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
    crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
    crossorigin="anonymous"></script>
</body>

</html>