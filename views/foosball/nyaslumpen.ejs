<% include ./partials/header.ejs %>
    <link rel="stylesheet" href="css/stats.css">
    <link rel="stylesheet" href="css/nyaslumpen.css">

    <!-- Register Page -->
    <div class="container">
        <div class="register">
            <h1>Nya slumpen</h1>
                <div class="player-container">
                <% data.forEach(player => { %>
                    <div class="play-choice">
                        <div class="choice-picture"><i class="fas fa-user fa-4x"></i></div>
                        <div><%= player.Spelare %></div>    
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
    <!-- //Register Page -->

